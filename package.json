{
  "name": "pushka-messanger",
  "version": "1.0.0",
  "description": "Учебный проект мессенджер Пушка",
  "scripts": {
    "dev": "NODE_ENV=development webpack serve --config webpack.dev.js",
    "build": "NODE_ENV=production webpack --progress --config webpack.prod.js",
    "eslint": "eslint './src/**/*.{js,ts}'",
    "stylelint": "stylelint '**/*{.css,.scss}'",
    "parcel:dev": "parcel src/index.html --no-source-maps --no-cache",
    "parcel:build": "parcel build src/index.html --no-cache",
    "parcel:start": "npm run parcel:build && node ./app.js"
  },
  "alias": {
    "api": "./src/api",
    "components": "./src/components",
    "core": "./src/core",
    "helpers": "./src/helpers",
    "pages": "./src/pages",
    "services": "./src/services",
    "utils": "./src/utils"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pushka-pashka/middle.messenger.praktikum.yandex.git"
  },
  "keywords": [
    "messanger"
  ],
  "author": "PushkarP",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pushka-pashka/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/pushka-pashka/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@parcel/transformer-inline-string": "2.8.2",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "autoprefixer": "10.4.13",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.7.3",
    "dotenv-webpack": "8.0.1",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "html-webpack-plugin": "5.5.0",
    "lightningcss": "1.18.0",
    "mini-css-extract-plugin": "2.7.3",
    "parcel": "2.8.2",
    "postcss": "8.4.20",
    "postcss-import": "15.1.0",
    "postcss-loader": "7.0.2",
    "prettier": "2.8.2",
    "style-loader": "3.3.1",
    "stylelint": "14.16.1",
    "stylelint-config-standard": "29.0.0",
    "ts-loader": "9.4.2",
    "typescript": "4.9.4",
    "webpack": "5.76.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "express": "4.18.2",
    "handlebars": "4.7.7",
    "nanoid": "4.0.0",
    "normalize.css": "8.0.1"
  }
}
